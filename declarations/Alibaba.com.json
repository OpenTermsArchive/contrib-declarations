{
  "name": "Alibaba.com",
  "importedFrom": "https://github.com/tosdr/tosback2/blob/f762cd4bbb2571272985fed009201a23300ba9b2/rules/alibaba.com.xml",
  "documents": {
    "Privacy Policy": {
      "fetch": "https://rule.alibaba.com/rule/detail/2034.htm",
      "select": [
        ".prompt",
        ".answer"
      ]
    },
    "Copyright Claims Policy": {
      "fetch": "https://rule.alibaba.com/rule/detail/6391.htm",
      "select": [
        ".prompt",
        ".answer"
      ]
    },
    "Terms of Service": {
      "fetch": "https://rulechannel.alibaba.com/icbu?type=detail&ruleId=2041&cId=1307#/rule/detail?cId=1307&ruleId=2041",
      "select": [
        "#app-root article#rule-detail-body"
      ],
      "remove": [
        ".zhi-tocItems",
        ".rule-text-explain",
        ".evalutaion"
      ],
      "executeClientScripts": true
    }
  }
}
