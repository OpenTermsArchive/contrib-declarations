{
  "name": "Kraken",
  "documents": {
    "Terms of Service": {
      "fetch": "https://www.kraken.com/en-us/legal#complete-tos",
      "select": {
        "startAfter": ".kraken-header",
        "endBefore": ".kraken-footer"
      },
      "executeClientScripts": true
    },
    "Privacy Policy": {
      "fetch": "https://www.kraken.com/en-us/legal/privacy/",
      "select": [
        ".kraken-article-body"
      ],
      "executeClientScripts": true
    },
    "Trackers Policy": {
      "fetch": "https://www.kraken.com/en-us/legal/cookies",
      "select": [
        ".kraken-article-body"
      ],
      "executeClientScripts": true
    }
  }
}
