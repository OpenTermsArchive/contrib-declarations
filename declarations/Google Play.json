{
  "name": "Google Play",
  "terms": {
    "Developer Terms": {
      "fetch": "https://play.google.com/intl/ALL_en/about/developer-distribution-agreement.html",
      "select": "body > .play-contained"
    },
    "Terms of Service": {
      "fetch": "https://play.google.com/intl/en_US/about/play-terms/index.html",
      "select": "#content"
    },
    "Brand Guidelines": {
      "fetch": "https://developer.android.com/distribute/marketing-tools/brand-guidelines",
      "filter": [
        "downloadImages"
      ],
      "select": "article.devsite-article"
    },
    "Review Guidelines": {
      "fetch": "https://support.google.com/googleplay/android-developer/answer/11044265?hl=en-IE",
      "select": "article.page",
      "remove": [
        ".scClientFeedbackInlineFeedbacklinkroot",
        ".article-survey-container"
      ]
    }
  }
}
