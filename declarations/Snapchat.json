{
  "name": "Snapchat",
  "terms": {
    "Terms of Service": {
      "fetch": "https://www.snap.com/terms",
      "select": ".sdsm-page",
      "filter": [
        "removeUTMfromUrls",
        "removeGclidFromUrls"
      ]
    },
    "Privacy Policy": {
      "fetch": "https://values.snap.com/privacy/privacy-policy",
      "select": ".mwp-page",
      "filter": [
        "removeUTMfromUrls",
        "removeGclidFromUrls"
      ]
    },
    "Trackers Policy": {
      "fetch": "https://www.snap.com/cookie-policy",
      "select": ".sdsm-page",
      "filter": [
        "removeUTMfromUrls",
        "removeGclidFromUrls"
      ]
    },
    "Community Guidelines": {
      "fetch": "https://values.snap.com/policy/policy-community-guidelines",
      "select": ".mwp-page",
      "remove": [
        ".sdsm-media"
      ],
      "filter": [
        "removeUTMfromUrls",
        "removeGclidFromUrls"
      ]
    }
  }
}
