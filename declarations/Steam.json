{
  "name": "Steam",
  "documents": {
    "Acceptable Use Policy": {
      "fetch": "https://store.steampowered.com/online_conduct/",
      "select": [
        "#ssa_box"
        ]
    },
    "Terms of Service": {
      "fetch": "https://store.steampowered.com/subscriber_agreement/",
      "select": [
        "#ssa_body"
      ],
      "remove": [
        "a[href=\"#0\"]"
      ]
    },
    "Privacy Policy": {
      "fetch": "https://store.steampowered.com/privacy_agreement/",
      "select": [
        "#main"
      ],
      "remove": [
        ".breadcrumbs",
        "#languages",
        "#pageTitle img"
      ]
    }
  }
}
