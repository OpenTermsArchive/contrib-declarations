{
  "name": "Snapchat",
  "terms": {
    "Terms of Service": {
      "fetch": "https://www.snap.com/terms",
      "select": ".sdsm-page",
      "filter": [
        {
          "removeQueryParams": [
            "gclid",
            "utm_campaign",
            "utm_medium",
            "utm_source"
          ]
        }
      ]
    },
    "Privacy Policy": {
      "fetch": "https://values.snap.com/privacy/privacy-policy",
      "select": ".mwp-page",
      "filter": [
        {
          "removeQueryParams": [
            "gclid",
            "utm_campaign",
            "utm_medium",
            "utm_source"
          ]
        }
      ]
    },
    "Trackers Policy": {
      "fetch": "https://www.snap.com/cookie-policy",
      "select": ".sdsm-page",
      "filter": [
        {
          "removeQueryParams": [
            "gclid",
            "utm_campaign",
            "utm_medium",
            "utm_source"
          ]
        }
      ]
    },
    "Community Guidelines": {
      "fetch": "https://values.snap.com/policy/policy-community-guidelines",
      "select": ".mwp-page",
      "remove": [
        ".sdsm-media"
      ],
      "filter": [
        {
          "removeQueryParams": [
            "gclid",
            "utm_campaign",
            "utm_medium",
            "utm_source"
          ]
        }
      ]
    }
  }
}