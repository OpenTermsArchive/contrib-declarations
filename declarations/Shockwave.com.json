{
  "name": "Shockwave.com",
  "importedFrom": "https://github.com/tosdr/tosback2/blob/f762cd4bbb2571272985fed009201a23300ba9b2/rules/shockwave.com.xml",
  "documents": {
    "Terms of Service": {
      "fetch": "http://www.shockwave.com/about/terms.jsp",
      "select": ".podContent"
    },
    "Privacy Policy": {
      "fetch": "http://www.shockwave.com/about/privacyPolicy.jsp",
      "select": [
        ".podContent"
      ],
      "filter": [
        "unifyEmailProtectedAddresses"
      ]
    }
  }
}
