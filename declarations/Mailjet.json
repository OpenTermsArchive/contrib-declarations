{
  "name": "Mailjet",
  "documents": {
    "Privacy Policy": {
      "fetch": "https://www.mailjet.com/privacy-policy/",
      "select": [
        "main"
      ]
    },
    "Terms of Service": {
      "fetch": "https://www.mailjet.com/terms-and-conditions/",
      "select": [
        ".section-content"
      ],
      "remove": [
        ".footer",
        ".subfooter"
      ]
    },
    "Acceptable Use Policy": {
      "fetch": "https://www.mailjet.com/legal/sending-policy/",
      "select": [
        "main"
      ]
    },
    "Trackers Policy": {
      "fetch": "https://www.mailjet.com/legal/privacy-policy/cookies/",
      "select": [
        {
          "startBefore": "h1",
          "endAfter": "main"
        }
      ],
      "remove": [
        "button"
      ]
    }
  }
}
