{
  "name": "Imgur",
  "documents": {
    "Privacy Policy": {
      "fetch": "https://imgur.com/privacy",
      "select": [
        ".text-content"
      ],
      "remove": [
        ".textbox > a"
      ]
   },
    "Community Guidelines": {
