{
  "name": "Cloudant",
  "importedFrom": "https://github.com/tosdr/tosback2/blob/f762cd4bbb2571272985fed009201a23300ba9b2/rules/cloudant.com.xml",
  "documents": {
    "Privacy Policy": {
      "fetch": "https://www.ibm.com/be-en/privacy?lnk=flg-priv-been",
      "select": [
        ".privacy-layout"
      ],
      "remove": [
        "aside",
        " .ibm-forward-link",
        " .ibm-anchor-up-link",
        " .ibm-rule.ibm-alternate.ibm-gray-30 + h2"
      ]
    },
    "Terms of Service": {
      "fetch": "https://www.ibm.com/be-en/legal?lnk=flg-tous-be-en",
      "select": "div#ibm-content-main > .node__content> div:first-child"
    }
  }
}
