{
  "name": "GitLab",
  "importedFrom": "https://github.com/tosdr/tosback2/blob/f762cd4bbb2571272985fed009201a23300ba9b2/rules/gitlab.com.xml",
  "terms": {
    "Privacy Policy": {
      "fetch": "https://about.gitlab.com/privacy/",
      "select": "main .content",
      "remove": [
        "#collapsible-sidebar",
        ".header-anchor"
      ],
      "filter": [
        "removeCloudflareEmailProtection"
      ]
    },
    "Vulnerability Disclosure Policy": {
      "fetch": "https://about.gitlab.com/security/disclosure/",
      "select": [
        ".container"
      ],
      "remove": [
        ".breadcrumb"
      ]
    },
    "Terms of Service": {
      "fetch": "https://about.gitlab.com/handbook/legal/policies/website-terms-of-use/",
      "select": "main .td-content",
      "remove": ".text-muted"
    }
  }
}
